{
	"private": true,
	"name": "woovi-playground",
	"scripts": {
		"build": "turbo run build",
		"config:local": "turbo run config:local",
		"dev": "turbo run dev",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"lint": "turbo run lint",
		"relay": "turbo run relay",
		"schema": "turbo run schema",
		"compose:up": "docker-compose -f docker-compose.yml up -d",
		"compose-down": "docker-compose -f docker-compose down"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.23.8",
		"@babel/preset-typescript": "^7.23.3",
		"@types/jest": "^29.5.14",
		"@types/mongodb-memory-server": "^1.8.0",
		"@types/mongoose": "^5.11.96",
		"@woovi-playground/eslint-config-custom": "*",
		"autoprefixer": "^10.4.16",
		"babel-plugin-transform-barrels": "^1.0.10",
		"dotenv": "^16.5.0",
		"eslint": "^8.53.0",
		"jest": "^29.7.0",
		"jest-junit": "^16.0.0",
		"mongodb-memory-server": "^10.1.4",
		"prettier": "^3.1.0",
		"ts-jest": "^29.3.2",
		"turbo": "latest"
	},
	"packageManager": "pnpm@10.0.0",
	"dependencies": {
		"graphql-ws": "^5.14.2",
		"mongoose": "^8.14.0"
	}
